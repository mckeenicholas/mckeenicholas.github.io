    "@typescript-eslint/parser": "8.33.1",
    "autoprefixer": "^10.4.21",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-svelte": "3.9.1",
    "postcss": "^8.5.4",
    "postcss-load-config": "6.0.1",
    "prettier": "^3.5.3",
